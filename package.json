{
  "name": "@primedeploy/mybu-expo-ui",
  "version": "0.1.9",
  "description": "UI components for MyBU app",
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/src/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.tsx",
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "prepare": "bob build",
    "release": "release-it --only-version"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lfellidev/mybu-expo-ui"
  },
  "author": "Prime Deploy <contact@primedeploy.com> (https://github.com/primedeploy/mybu-expo-ui.git)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lfellidev/mybu-expo-ui/issues"
  },
  "homepage": "https://github.com/lfellidev/mybu-expo-ui#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@expo-google-fonts/baloo-2": "^0.4.2",
    "@expo-google-fonts/be-vietnam-pro": "^0.4.1",
    "@expo-google-fonts/inter": "^0.4.2",
    "@expo-google-fonts/lexend": "^0.4.1",
    "@expo-google-fonts/nunito": "^0.4.2",
    "@expo-google-fonts/poppins": "^0.4.1",
    "@expo-google-fonts/quicksand": "^0.4.1",
    "@expo-google-fonts/roboto": "^0.4.2",
    "@expo-google-fonts/sora": "^0.4.2",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-masked-view/masked-view": "0.3.2",
    "axios": "^1.13.2",
    "expo-app-loading": "^1.0.3",
    "expo-application": "~7.0.8",
    "expo-battery": "~10.0.8",
    "expo-constants": "~18.0.11",
    "expo-device": "~8.0.10",
    "expo-font": "~14.0.10",
    "expo-keep-awake": "~15.0.8",
    "expo-linking": "~8.0.10",
    "expo-localization": "~17.0.8",
    "expo-network": "~8.0.8",
    "expo-router": "~6.0.17",
    "expo-splash-screen": "~31.0.12",
    "expo-status-bar": "~3.0.9",
    "expo-updates": "~29.0.15",
    "lottie-react-native": "~7.3.1",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-performance": "^6.0.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-version-check": "^3.5.0"
  },
  "peerDependencies": {
    "expo": "~54.0.27",
    "react": "19.1.0",
    "react-native": "0.81.5"
  },
  "devDependencies": {
    "@react-native/typescript-config": "^0.82.1",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.21",
    "@types/react": "^19.2.7",
    "@types/react-native": "^0.73.0",
    "@types/react-native-version-check": "^3.4.8",
    "@types/react-test-renderer": "^19.1.0",
    "react-native-builder-bob": "^0.40.16",
    "typescript": "^5.9.3"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "create-react-native-library": {
    "languages": "js",
    "type": "library",
    "version": "0.1.9"
  },
  "private": false
}
