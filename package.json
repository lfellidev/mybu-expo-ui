{
	"name": "@primedeploy/mybu-expo-ui",
	"version": "0.0.75",
	"description": "UI components for MyBU app",
	"main": "./lib/module/index.js",
	"types": "./lib/typescript/src/index.d.ts",
	"exports": {
		".": {
			"source": "./src/index.tsx",
			"types": "./lib/typescript/src/index.d.ts",
			"default": "./lib/module/index.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"src",
		"lib",
		"android",
		"ios",
		"cpp",
		"*.podspec",
		"react-native.config.js",
		"!ios/build",
		"!android/build",
		"!android/gradle",
		"!android/gradlew",
		"!android/gradlew.bat",
		"!android/local.properties",
		"!**/__tests__",
		"!**/__fixtures__",
		"!**/__mocks__",
		"!**/.*"
	],
	"scripts": {
		"prepare": "bob build",
		"release": "release-it --only-version"
	},
	"keywords": [
		"react-native",
		"ios",
		"android"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/lfellidev/mybu-expo-ui"
	},
	"author": "Prime Deploy <contact@primedeploy.com> (https://github.com/primedeploy/mybu-expo-ui.git)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/lfellidev/mybu-expo-ui/issues"
	},
	"homepage": "https://github.com/lfellidev/mybu-expo-ui#readme",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"devDependencies": {
		"@react-native/babel-preset": "0.81.1",
		"@types/react": "^19.1.12",
		"react": "19.1.0",
		"react-native": "0.81.4",
		"react-native-builder-bob": "^0.40.13",
		"typescript": "^5.9.2",
		"@types/react-native-version-check": "^3.4.8"
	},
	"dependencies": {
		"react": "*",
		"react-native": "*",
		"@expo/vector-icons": "^15.0.2",
		"axios": "^1.12.2",
		"expo-constants": "~18.0.8",
		"expo-device": "~8.0.7",
		"expo-font": "~14.0.8",
		"expo-network": "~8.0.7",
		"expo-status-bar": "~3.0.8",
		"expo-updates": "~29.0.10",
		"react-native-iphone-x-helper": "^1.3.1",
		"react-native-svg": "15.12.1",
		"react-native-version-check": "^3.5.0",
		"expo-application": "~7.0.7"
	},
	"react-native-builder-bob": {
		"source": "src",
		"output": "lib",
		"targets": [
			[
				"module",
				{
					"esm": true
				}
			],
			[
				"typescript",
				{
					"project": "tsconfig.build.json"
				}
			]
		]
	},
	"create-react-native-library": {
		"languages": "js",
		"type": "library",
		"version": "0.54.3"
	},
	"private": false
}
